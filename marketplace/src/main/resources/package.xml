<package type="addon" name="amazon-s3-direct-upload" version="@VERSION@">
  <title>Amazon S3 Direct Upload for Web UI</title>
  <description>The Amazon S3 Direct Upload for Web UI allows Web UI to make use of the Amazon S3 Online Storage addon for file storage.</description>
  <home-page>https://doc.nuxeo.com/nxdoc/amazon-s3-online-storage/</home-page>
  <platforms>
    <platform>server-@DISTRIB_VERSION@</platform>
    <platform>server-@DISTRIB_VERSION@-*</platform>
  </platforms>
  <dependencies>
    <package>nuxeo-web-ui</package>
    <package>amazon-s3-online-storage</package>
  </dependencies>
  <hotreload-support>false</hotreload-support>
  <supported>true</supported>
  <production-state>production_ready</production-state>
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <vendor>Nuxeo</vendor>
  <license>Apache License, Version 2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0.html</license-url>
  <visibility>MARKETPLACE</visibility>
</package>
